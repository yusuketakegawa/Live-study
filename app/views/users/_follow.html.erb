<ul class="collection follow">
  <div id="test-swipe-1" class="col s12 ">
    <% follow__users.each do |user|%>
      <li class="collection-item follow__item">
        <%= link_to user_path(user.id) do %>
          <div class="follow__avatar">
            <%= image_tag user.image.url, class: "follow__avatar__pic "%>
              <div class="follow__avatar__name">
                <%= user.nickname%>
              </div>  
          </div>  
            <% if current_user != user %>
              <% if user.followed_by?(current_user) %>
                <p><%=link_to "フォロー中", user_relationships_path(user.id), method: :delete, class: "btn" %></p>
              <% else %>
                <p><%=link_to "フォローする", user_relationships_path(user.id), method: :post, class: "btn" %></p>
              <% end %>
            <% end %>
        <% end %>
      </li>
    <%end %>
  </div>
</ul>
